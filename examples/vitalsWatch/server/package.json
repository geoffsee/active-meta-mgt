{
  "name": "active-meta-mgt-example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run src/server.ts",
    "start": "bun run src/server.ts",
    "probe": "bun run src/main.ts",
    "test": "bun test",
    "test:integration": "bun test src/__tests__/integration",
    "test:watch": "../client/scripts/run-watch-tests.sh",
    "dev:worker": "wrangler dev",
    "deploy": "wrangler deploy",
    "cf:kv:create": "wrangler kv:namespace create PATIENTS_KV && wrangler kv:namespace create PATIENTS_KV --preview",
    "cf:secret:set": "wrangler secret put OPENAI_API_KEY",
    "fmt": "bunx prettier -w ."
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 2
  },
  "dependencies": {
    "active-meta-mgt": "file:..",
    "csv-parse": "^6.1.0",
    "hono": "^4.4.0",
    "openai": "^4.31.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@types/node": "^22.9.0",
    "typescript": "^5.6.2",
    "wrangler": "^3.57.0"
  }
}
